<!doctype html>
<html>
<head>{{> head }}</head>
<body data-app="{{ appUrl }}" data-appRoot="{{ appRoot }}" class="{{ bodyClass }}">
  {{> header }}
  <div class="container-fluid"><div class="flex-container">
     {{#content}}
    <main class="main hasSidebar" role="main">
    {{#title}}
      <h2  class="page-title">{{{html}}}</h2>
    {{/title}}
   
      {{#main}}
        <div class="maintext {{class}}" >{{{html}}}</div>
      {{/main}}
   
    </main>
    <aside role="complementary">
      {{#aside}}
         <h3>{{title}}</h3>
         <div class="content">{{{html}}}</div>
       {{/aside}}
    </aside>
 {{/content}}
 </div>

</div>
  {{> footer }}
  {{{ closure }}}  
</body>
</html>
