<!doctype html>
<html class="nojs">
<head>
  {{> head }}
  <link rel="stylesheet" type="text/css" href="{{appRoot}}/css/upgrade.css">
</head>
<body data-app="{{appUrl}}" data-appRoot="{{appRoot}}" class="{{bodyClass}}" data-script='{ "js" : [ "commons.js" ] }'>
  {{> header }}  
  <div class="container-fluid">
    <div class="flex-container">
      {{#content}}
        <main class="main hasSidebar" role="main">
        {{#main}}
          <h2  class="page-title">{{title}}</h2>
          <div class="maintext">{{{html}}}</div>
        {{/main}}
        </main>
      {{/content}}
    </div>
  </div>
  {{> footer }}
  {{{ closure }}}  
</body>
</html>