{{#featured}}
  <div id="{{id}}" class="noFouc">
    <ul>
      {{#raw}}<li><h3><a href="#{{label}}">{{#widget}}{{title}}{{/widget}}</a></h3></li>{{/raw}}
    </ul>
    <div>
      {{#raw}}
        <div id="{{label}}">
          <div class="item-list flex-container">
            {{#data}}
              {{#response}}
                {{#each docs}}
                  <article class="item card">
                    <div class="thumbs">
                      <div class="clipper"><a href="{{appRoot}}/book/{{ss_identifer}}"><img src="{{ss_image_medium}}" alt="" title="{{title}}"/></a></div>
                    </div>
                    <h1 class="md_title"><a href="{{appUrl}}/book/{{ss_identifer}}">{{{ss_title}}}</a></h1>
                    <div class="md_author"><span>{{sm_author}}</span></div>
                    {{#if is_ispartof_series}}
                      <div class="md_series"><span class="md_label">Series</span> <span>{{ss_series_label}}</span></div>
                    {{/if}}
                    {{#if is_ispartof_multivol}}
                      <div class="md_multivol"><span class="md_label">Part of</span> <span>{{ss_multivol_label}}</span></div>
                    {{/if}}
                    <div class="md_publisher"><span class="md_label">Publisher:</span> <span>{{sm_publisher}}</span>, {{ss_publocation}}, {{ss_pubdate}}</div>
                    <div class="md_provider"><span class="md_label">Provider:</span> <span>{{sm_partner}}</span></div>
                    <div class="md_subjects"><span class="md_label">Subjects:</span> {{#sm_subject}}<a class="md_subject" href="{{appRoot}}/search?q={{.}}">{{.}}</a>{{/sm_subject}}</div>
                  </article>  
                {{/each}}
              {{/response}}
            {{/data}}
            <article class="item"></article>
            <article class="item"></article>
          </div>
        </div>
      {{/raw}}
    </div>
  </div>
{{/featured}}
